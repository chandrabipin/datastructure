package com.bchandra.datastructure;

public class OO4CircularDoubleLinkedListNode {

	private OO4CircularDoubleLinkedListNode next = null;
	private OO4CircularDoubleLinkedListNode previous = null;
	private Integer value = null;
	/**
	 * @return the next
	 */
	public OO4CircularDoubleLinkedListNode getNext() {
		return next;
	}
	/**
	 * @param next the next to set
	 */
	public void setNext(OO4CircularDoubleLinkedListNode next) {
		this.next = next;
	}
	/**
	 * @return the value
	 */
	public Integer getValue() {
		return value;
	}
	/**
	 * @param value the value to set
	 */
	public void setValue(Integer value) {
		this.value = value;
	}
	/**
	 * @return the previous
	 */
	public OO4CircularDoubleLinkedListNode getPrevious() {
		return previous;
	}
	/**
	 * @param previous the previous to set
	 */
	public void setPrevious(OO4CircularDoubleLinkedListNode previous) {
		this.previous = previous;
	}
}
